{
  "name": "webpack5-scss-js",
  "version": "1.0.0",
  "description": "webpack5 scss js template",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode=development --config webpack.dev.config.js",
    "devserver": "cross-env NODE_ENV=development webpack-dev-server --mode=development --config webpack.devserver.config.js --open",
    "devwatch": "cross-env NODE_ENV=development webpack --mode=development --config webpack.dev.config.js --watch",
    "prod": "cross-env NODE_ENV=production webpack --mode=production --config webpack.prod.config.js",
    "stats": "cross-env NODE_ENV=production webpack --mode=production --config webpack.analyzer.config.js"
  },
  "keywords": [
    "webpack",
    "webpack5",
    "scss",
    "js"
  ],
  "author": "mpwdev",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "csv-loader": "^3.0.5",
    "eslint": "^8.42.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "papaparse": "^5.4.1",
    "postcss-load-config": "^4.0.1",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^8.4.2",
    "sass": "^1.63.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.7.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.86.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "xml-loader": "^1.2.1"
  }
}
